<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Naresh" >
<listeners>
        <listener class-name="Components.Listeners"/>
</listeners>
       <test name="ParameterizedTestMethodCash">
        <classes>
            <class name="Enquiry.Enrolling_Student">
                <methods>
                    <include name="Enrolling">
                        <parameter name="Options" value="Cash" />
                    </include>
                </methods>
            </class>
        </classes>
    </test>
    
    <test name="ParameterizedTestMethodDDCheque">
        <classes>
            <class name="Enquiry.Enrolling_Student">
