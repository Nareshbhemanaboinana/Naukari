<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Naresh" >
<listeners>
        <listener class-name="Components.Listeners"/>
</listeners>
       <test name="ParameterizedTestMethodCash">
        <classes>
            <!-- Include the class for the dependent method -->
            <class name="Enquiry.Adding_college_level_Enquiry">
                <methods>
                    <include name="Creating_college_level_Enquiry"/>
                </methods>
            </class>
            <!-- Include the class for the main method -->
            <class name="Enquiry.Enrolling_Student_with_all_Modeofpayments">
                <methods>
                    <include name="Enrolling_with_All_ModeofPayments"/>
                    <parameter name="Options" value="Cash"/>
     					
  
     </methods>
	
            </class>
        </classes>
    </test>
     
